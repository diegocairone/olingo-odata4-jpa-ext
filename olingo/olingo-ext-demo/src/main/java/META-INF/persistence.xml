<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="olingo-ext-demo" transaction-type="RESOURCE_LOCAL">
		<class>com.cairone.olingo.ext.demo.converters.CheckTypeConverter</class>
		<class>com.cairone.olingo.ext.demo.converters.GenderConverter</class>
		<class>com.cairone.olingo.ext.demo.converters.LocalDateConverter</class>
		<class>com.cairone.olingo.ext.demo.entities.CountryEntity</class>
		<class>com.cairone.olingo.ext.demo.entities.CustomerEntity</class>
		<class>com.cairone.olingo.ext.demo.entities.FormEntity</class>
		<class>com.cairone.olingo.ext.demo.entities.FormTypeEntity</class>
		<class>com.cairone.olingo.ext.demo.entities.PersonCheckLogEntity</class>
		<class>com.cairone.olingo.ext.demo.entities.PersonEntity</class>
		<class>com.cairone.olingo.ext.demo.entities.RegionEntity</class>
		<class>com.cairone.olingo.ext.demo.entities.StateEntity</class>
		<class>com.cairone.olingo.ext.demo.entities.UserEntity</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/olingo-ext-demo"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<property name="javax.persistence.jdbc.password" value="asd123"/>
		</properties>
	</persistence-unit>
</persistence>
